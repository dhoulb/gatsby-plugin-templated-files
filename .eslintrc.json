{
	"parser": "babel-eslint",
	"plugins": [
		"json"
	],
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended",
		"plugin:react/recommended",
		"plugin:import/errors",
		"plugin:import/warnings"
	],
	"env": {
		"node": true,
		"es6": true,
		"jest": true
	},
	"globals": {
		"cy": true
	},
	"rules": {
		"eqeqeq": 2,
		"no-alert": 2,
		"no-caller": 2,
		"no-console": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-floating-decimal": 2,
		"no-implicit-globals": 2,
		"no-labels": 2,
		"no-new-require": 2,
		"no-param-reassign": 2,
		"no-path-concat": 2,
		"no-shadow": 2,
		"no-template-curly-in-string": 2,
		"no-useless-rename": 2,
		"no-var": 2,
		"prefer-const": 2,
		"prefer-spread": 2,
		"react/prop-types": 0
	}
}